{
  "name": "GOJO - გოჯო სატორი",
  "version": "1.0",
  "options_page": "settings.html",
  "manifest_version": 3,
  "action": {
    "default_title": "GOJO - გოჯო სატორი",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/faicon16.png",
      "32": "icons/favicon32.png"
    }
  },
  "icons": {
    "16": "icons/favicon16.png",
    "32": "icons/favicon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "ბრაუზერის ავტომატიზაციისათვის",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js"],
      "run_at": "document_start"
    },
    {
      "matches": [
        "http://file.ge/*",
        "https://ipoc.ge/*",
        "https://facebook.com/*",
        "https://mykadri.com/*",
        "https://adjaranet.com/*",
        "https://google.ge/*",
        "https://instagram.com/*",
        "https://github.com/*",
        "https://adjaranet.com/*"
      ],
      "html": ["display.html"],
      "js": ["script.js"],
      "css": ["mystyle.css"],
      "run_at": "document_start"
    }
  ]
}
